{
  "manifest_version": 2,
  "name": "The Shaqtivator",
  "version": "1.0",
  "description": "Shaqtivate everything.",
  "permissions": ["storage"],
    "icons" : {
	"16": "icon16.png"
	"48": "icon48.png"
	"128": "icon128.png"      
    },
  "content_scripts": [
    {
        "matches": [
            "*://*/*"
        ],
        "js": [
            "content.js"
        ],
        "run_at": "document_end"
    }
]
}
